    "@supabase/supabase-js": "^2.38.2",
    "@types/react-datepicker": "^4.15.1",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.5.0",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.1",
    "electron-updater": "^6.1.4",
    "moment": "^2.29.4",
    "portfinder": "^1.0.32",
    "react": "^18.2.0",
    "react-datepicker": "^4.18.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
